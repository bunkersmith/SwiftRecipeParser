            <unitOfMeasure>lb</unitOfMeasure>
            <ingredientName>Boneless Skinless Chicken Breasts</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1/2</quantity>
            <unitOfMeasure>c</unitOfMeasure>
            <ingredientName>Coconut Milk</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
