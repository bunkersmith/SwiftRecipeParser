            <processingInstructions> no salt added</processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>2</quantity>
            <unitOfMeasure>tbsp</unitOfMeasure>
            <ingredientName>reduced-sodium soy sauce</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>2</quantity>
            <unitOfMeasure>tsp</unitOfMeasure>
            <ingredientName>cornstarch</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>tsp</unitOfMeasure>
            <ingredientName>toasted sesame oil</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
