        <ingredient>
            <quantity>1-1/10</quantity>
            <unitOfMeasure>lb</unitOfMeasure>
            <ingredientName>Beef, Pork or Chicken</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>Onion</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>Garlic clove</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>2</quantity>
            <unitOfMeasure>tbsp</unitOfMeasure>
            <ingredientName>Kecap Manis (See Recipe Below)</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>tsp</unitOfMeasure>
            <ingredientName>Coriander powder</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>tsp</unitOfMeasure>
            <ingredientName>Cummin powder</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
