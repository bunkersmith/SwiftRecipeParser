<?xml version="1.0" encoding="UTF-8"?>
<recipe>
    <name>Jalapeno Cornbread</name>
    <indexCharacter>J</indexCharacter>
    <notes></notes>
    <servings>4</servings>
    <instructions>In a mixing bowl, mix together cornbread mix, milk and eggs. Add corn, jalapenos, cheese and onion. Heat oil and add to mixture. Pour into an 8 inch pan and bake at 375 degrees for 45 minutes.</instructions>
    <ingredients>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>c</unitOfMeasure>
            <ingredientName>cornbread mix</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>7/8</quantity>
            <unitOfMeasure>c</unitOfMeasure>
            <ingredientName>milk</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>eggs, beaten</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1/3</quantity>
            <unitOfMeasure>c</unitOfMeasure>
            <ingredientName>whole kernel corn, drained</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>2-2/3</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>sm. jalapenos, chopped</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>7/8</quantity>
            <unitOfMeasure>c</unitOfMeasure>
            <ingredientName>grated sharp cheddar cheese</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1/3</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>lg. onion, chopped</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>2</quantity>
            <unitOfMeasure>T</unitOfMeasure>
            <ingredientName>salad oil or bacon grease</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
    </ingredients>
</recipe>
