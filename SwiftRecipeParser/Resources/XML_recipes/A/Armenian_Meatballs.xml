            <quantity>1</quantity>
            <unitOfMeasure>lb</unitOfMeasure>
            <ingredientName>ground lamb</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>egg</ingredientName>
            <processingInstructions>beaten</processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1/2</quantity>
            <unitOfMeasure>t</unitOfMeasure>
            <ingredientName>salt</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>ea</unitOfMeasure>
            <ingredientName>dash black pepper</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1/4</quantity>
            <unitOfMeasure>t</unitOfMeasure>
            <ingredientName>garlic salt</ingredientName>
            <processingInstructions></processingInstructions>
        </ingredient>
        <ingredient>
            <quantity>1</quantity>
            <unitOfMeasure>c</unitOfMeasure>
            <ingredientName>shredded Cheddar cheese</ingredientName>
